{
  "name": "geo-conversion-api",
  "version": "0.0.1",
  "description": "This API converts geometry information from and to various formats.",
  "type": "module",
  "exports": [
    "./server.js"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "postbuild": "copyfiles api/**/*.json **/*.html **/*.xml dist",
    "install-production": "NODE_ENV=production npm ci",
    "postinstall": "patch-package",
    "dev": "tsx watch ./server.ts",
    "test": "FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --import tsx --test-reporter ./test/reporter.js --test ./test/**/*.ts"
  },
  "keywords": [
    "geojson",
    "wkt",
    "gml",
    "api",
    "conversion",
    "gdal",
    "ogr2ogr"
  ],
  "author": "Erik-LÃ¢n Do Dinh",
  "license": "EUPL",
  "dependencies": {
    "@fastify/autoload": "^6.0.2",
    "@fastify/swagger": "^9.2.0",
    "@scalar/fastify-api-reference": "^1.25.64",
    "@sinclair/typebox": "^0.34.4",
    "@xmldom/xmldom": "^0.9.4",
    "fastify": "^5.0.0",
    "marked": "^15.0.1",
    "marked-gfm-heading-id": "^4.1.0",
    "ogr2ogr": "^5.1.0",
    "patch-package": "^8.0.0",
    "read-config-ng": "^3.0.7",
    "xml-formatter": "^3.6.3",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.11.30",
    "copyfiles": "^2.4.1",
    "eslint": "^9.15.0",
    "eslint-plugin-license-header": "^0.6.1",
    "lodash": "^4.17.21",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  }
}
